//This is new feature of Java SE 8 version ..will not work in Java SE 7
import java.time.LocalDate;

///////////////////////////////////logic///////////////////////////////////////////////

class Result {
    public static String findDay(int month, int day, int year) {
        LocalDate ldt=LocalDate.of(year,month,day);
        String str=ldt.getDayOfWeek().name();
        return str;
    }
}

///////////////////////////////////////////////////////////////////////////////////////
